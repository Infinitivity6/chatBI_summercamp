{
  "name": "chatbi",
  "version": "0.1.0",
  "description": "ChatBI is a BI system interacted by chat.",
  "main": "index.js",
  "scripts": {
    "dev:server": "pnpm run -C server dev",
    "dev:client": "pnpm run -C client dev",
    "format": "prettier --write \"**/src/**/*.ts\" \"server/test/**/*.ts\"",
    "lint": "eslint \"**/{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install"
